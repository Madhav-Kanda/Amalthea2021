<link rel="stylesheet" href="../css/Webinar.css">

<section class="text-white body-font Webinarbg">
    <div class="container px-5 py-20 mx-auto">
        <div class="flex flex-wrap flex-col mt-10 mx-16">
            <div class="lg:w-1/2 w-full mb-6 lg:mb-0">
                <h1 class="sm:text-6xl text-6xl font-bold title-font text-white sora">WEBINARS</h1>
            </div>
        </div>
    </div>
</section>
<hr>
<section id="UpcomingWebinars" class="text-white bgnewblack mb-1">
    <div class="lg:w-1/2 w-full lg:mb-0 mx-6 py-4">
        <h1 class="sm:text-4xl text-3xl font-medium title-font text-white mx-16 py-16">
            <span class="underline_ ">
                UPCOMING WEBINARS
            </span>
        </h1>
    </div>
    <div class="flex flex-wrap mx-4 justify-evenly">
        <% webinars1.forEach(webinar=> { %>
            <div class="xl:w-1/4 md:w-1/3 p-4 mx-1 bg-gray-500 my-6 rounded-md">
                <img src="<%= webinar.image %>" alt="">
                <div class="flex justify-center">
                    <button class="bg-black py-2 px-4 mt-4 rounded-md">Know More</button>
                </div>
            </div>
            <% }) %>
    </div>
</section>

<section id="CompletedWebinar" class="text-white bgnewblack mb-1">
    <div class="lg:w-1/2 w-full mb-6  lg:mb-0 mx-20 py-16">
        <h1 class="sm:text-4xl text-3xl font-medium title-font mb-2 text-white mx-2">
            <span class="underline_">
                COMPLETED WEBINARS
            </span>
        </h1>
    </div>
    <div class="flex flex-wrap mx-4 justify-evenly">
        <% webinars2.forEach(webinar=> { %>
            <div class="xl:w-1/4 md:w-1/3 p-4 pb-6 mx-1 bg-gray-500 my-6 rounded-md">
                <img src="<%= webinar.image %>" alt="">
                <div class="flex justify-between">
                    <button class="bg-black py-2 px-3 mt-6 mr-1 rounded-md">Know More</button>
                    <button class="bgnewblue py-2 px-3 mt-6 ml-1 rounded-md">View on Youtube</button>
                </div>
            </div>
            <% }) %>
    </div>
</section>